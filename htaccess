

# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:blanctech.co.uk
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?blanctech.co.uk
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
